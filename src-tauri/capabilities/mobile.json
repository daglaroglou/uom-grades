{
  "$schema": "../gen/schemas/mobile-schema.json",
  "identifier": "mobile-capability",
  "platforms": ["android"],
  "windows": ["main"],
  "permissions": [
    "haptics:default",
    "biometric:default",
    "opener:default",
    {
      "identifier": "opener:allow-open-url",
      "allow": [
        { "url": "https://github.com/**" },
        { "url": "https://sis-portal.uom.gr/**" }
      ]
    }
  ]
}
